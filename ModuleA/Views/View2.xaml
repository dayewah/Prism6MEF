<UserControl x:Class="ModuleA.Views.View2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <DockPanel Background="Azure">
        <Button DockPanel.Dock="Top" HorizontalAlignment="Center" Command="{Binding NavigateCommand}" CommandParameter="ModuleAView">Nav</Button>
        <Label Content="{Binding Name}"  FontSize="100" VerticalAlignment="Center" HorizontalAlignment="Center" />
    </DockPanel>
</UserControl>

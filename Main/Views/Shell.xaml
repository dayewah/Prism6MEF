<Window x:Class="Main.Views.Shell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <DockPanel>
        <DockPanel>
            <Label DockPanel.Dock="Top" Height="25" Content="{Binding Name}"/>
        <StackPanel DockPanel.Dock="Left" Background="#FF202020" Width="60">
            
        </StackPanel>
        <ContentControl prism:RegionManager.RegionName="MainRegion"></ContentControl>
        </DockPanel>
    </DockPanel>
</Window>
